{
  "expo": {
    "name": "RETALIA",
    "slug": "retalia",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.retalia.app"
    },
    "android": { 
      "package": "com.retalia.app"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
